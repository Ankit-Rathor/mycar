<h1>Welcome to <%= current_user.name %> </h1>
<h1>Booking Data</h1>
<table>
  <tr>
    <th>Owner Name</th>
    <th>Email</th>
    <th>Contact</th>
    <th>City</th>
    <th>Location</th>
    <th>House_number</th>
    <th>Car</th>
    <th>Model</th>
    <th>Car_number</th>
    <th>Issue</th>
    <th>Service_details</th>
  </tr>
  <% @user_booking.each do |user_booking|%>
    <tr> 
      <td><%= user_booking.user.name %></td>
      <td><%= user_booking.user.email %></td>
      <td><%= user_booking.user.contact %></td>
      <td><%= user_booking.user.address.city %></td>
      <td><%= user_booking.user.address.location%></td>
      <td><%= user_booking.user.address.house_no %></td>
      <td><%= user_booking.car.name %></td>
      <td><%= user_booking.car.number %></td>
      <td><%= user_booking.car.model %></td>
      <td><%= user_booking.issue %></td>
      <td> <%= link_to "service_details", service_details_path %></td>

  <% end %>
    </tr>
</table>

